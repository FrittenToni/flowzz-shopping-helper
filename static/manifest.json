{
    "manifest_version": 3,
    "name": "Flowzz Shopping Helper",
    "version": "1337.0",
    "permissions": [
      "storage",
      "activeTab",
      "tabs"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
      }
    },
    "host_permissions": [
      "https://flowzz.com/*"
    ]
  }
  